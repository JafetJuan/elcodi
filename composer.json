        "symfony/expression-language": "^3.2",
        "symfony/finder": "^3.2",
        "symfony/browser-kit": "^3.2",
        "symfony/routing": "^3.2",
        "symfony/proxy-manager-bridge": "^3.2",
        "symfony/twig-bundle": "^3.2",
        "symfony/swiftmailer-bundle": "^2.4",
        "symfony/form": "^3.2",
        "twig/twig": "^1.28",
        "ocramius/proxy-manager": "^2.0",

        "doctrine/common": "dev-master as 2.6.2",
        "doctrine/orm": "dev-master",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "mmoreram/simple-doctrine-mapping": "^1.0",
        "mmoreram/symfony-bundle-dependencies": "^1.1.1",
        "mmoreram/base-bundle": "dev-master",

        "moneyphp/money": "^3.0",
        "knplabs/knp-gaufrette-bundle": "^0.3.0",
