        "elcodi/comment": "self.version",
        "elcodi/tax": "self.version",
        "elcodi/cart-shipping": "self.version",
        "elcodi/shipping": "self.version",
        "elcodi/state-transition-machine": "self.version",
        "elcodi/page": "self.version",
        "elcodi/entity-translator": "self.version",
        "elcodi/sitemap": "self.version",
        "elcodi/metric": "self.version",
        "elcodi/plugin": "self.version",
        "elcodi/payment": "self.version",
        "elcodi/store": "self.version"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^2.2",
        "friendsofphp/php-cs-fixer": "1.12.4",
        "mmoreram/php-formatter": "dev-master",
        "phpunit/phpunit": "^5.6.5"
    },
    "autoload": {
        "exclude-from-classmap": [
            "src/**/Tests/"
        ],
        "psr-4": {
            "Elcodi\\": "src/Elcodi"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "scripts": {
        "check-style": [
            "vendor/bin/php-cs-fixer fix --config-file=.php_cs || true",
            "vendor/bin/php-formatter f:h:f src || true",
            "vendor/bin/php-formatter f:s:f src || true",
            "vendor/bin/php-formatter f:u:s src || true"
        ],
        "test": "vendor/bin/phpunit"
    }
}
